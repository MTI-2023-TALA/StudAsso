    "@angular/language-service": "^13.3.3",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@nrwl/angular": "^13.10.3",
    "@nrwl/cli": "^13.10.3",
    "@nrwl/cypress": "^13.10.3",
    "@nrwl/eslint-plugin-nx": "13.10.1",
    "@nrwl/jest": "^13.10.3",
    "@nrwl/linter": "^13.10.3",
