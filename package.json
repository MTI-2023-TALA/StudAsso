  "scripts": {
    "start": "nx serve",
    "build": "nx run backend:build:production",
    "test": "nx test",
    "cd": "node dist/apps/backend/main.js",
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2015 browser module main",
    "migration:create": "npx typeorm migration:create -f libs/backend/core/migrations/src/lib/ormconfig.local.ts",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -f libs/backend/core/migrations/src/lib/ormconfig.local.ts",
    "migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert -f libs/backend/core/migrations/src/lib/ormconfig.local.ts",
    "docker": "docker-compose -f docker-compose.yml up"
  },
